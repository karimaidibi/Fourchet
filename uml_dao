@startuml
class User {
  -name: String
  -email: String
  -password: String
}

interface DAO<T> {
  +get(long id) : Optional<T> 
  +getAll() : List<T> 
  +save(T t) : void 
  +update(T t, String[] params) : void 
  +delete(T t) : void 
}

class UserDao{   
  -users : List<User>
}

class UserApplication {
  -userDao : DAO
}

DAO <|.. UserDao
UserDao - User
UserApplication -> UserDao
@enduml
